{
    "all": true,
    "include": ["src/**/*.js", "src/**/*.jsx"],
    "exclude": ["**/*.test.js", "**/*.test.jsx"],
    "reporter": ["text", "html"],
    "reportDir": "./coverage",
    "instrument": true,
    "per-file": true,
    "branches": 80,
    "functions": 80,
    "lines": 80,
    "statements": 80
}
